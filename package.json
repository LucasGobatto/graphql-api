{
  "name": "graphql-api",
  "version": "1.0.0",
  "description": "GraphQL server template",
  "main": "src/index.ts",
  "scripts": {
    "dev": "nodemon src/index.ts --exec ts-node src/index.ts -e ts,graphql",
    "build": "tsc",
    "start": "npm run build && node dist/index.js",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "docker": "docker-compose up -d",
    "test": "set TEST=OK && echo \"Error: Create a test script\" && exit 1"
  },
  "dependencies": {
    "apollo-server-express": "2.24.0",
    "dotenv": "9.0.1",
    "express": "4.17.1",
    "graphql": "15.5.0",
    "jsonwebtoken": "8.5.1",
    "pg": "8.6.0",
    "reflect-metadata": "0.1.13",
    "typedi": "0.10.0",
    "typeorm": "0.2.32",
    "typeorm-typedi-extensions": "0.4.1"
  },
  "devDependencies": {
    "@types/express": "4.17.11",
    "@types/jsonwebtoken": "8.5.1",
    "@types/node": "15.14.9",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "eslint": "7.26.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "nodemon": "2.0.7",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/LucasGobatto/graphql-api.git"
  },
  "author": "LucasGobatto",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LucasGobatto/graphql-api/issues"
  },
  "homepage": "https://github.com/LucasGobatto/graphql-api#readme"
}
